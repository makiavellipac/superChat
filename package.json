    "postcss-cssnext": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "simple-git-hooks": "^2.7.0",
    "terser-webpack-plugin": "^5.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --allow-empty"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write **/*.{ts,tsx,js,jsx,json}",
      "eslint . --ext .ts,.js,.tsx,.jsx"
    ]
